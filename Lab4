<script type = "text/javascript">
var x, y;
x = parseInt(prompt("Введите значение х",''));
y = parseInt(prompt("Введите значение у",''));
if (x == y){
alert("Введенные числа равны!");
}
else if(x < y){
alert("Максимальное число - y");
}
else{
alert("Максимальное число - x");
}
var password, p;
password = "12345";
pass = prompt("Введите правильный пароль",'');
if (password == pass){
alert("Пароль верен");
}
else{
alert("Неправильно введенный пароль");
}
document.writeln("Пароль был вот таким : " + password +". Неповезло - неповезло");
</script>
